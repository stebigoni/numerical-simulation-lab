    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "data = np.loadtxt(fname = \"output.dat\")\n",
    "\n",
    "r = data[:,0]\n",
    "err = data[:,1]\n",
    "x = np.arange(100)\n",
    "plt.errorbar(x,r-0.5,yerr=err)\n",
    "plt.title(\"Estimation of $<r>$ \")\n",
    "plt.xlabel(\"Block #\")\n",
    "plt.ylabel(\"$<r>$-1/2\")\n",
    "plt.show()"
