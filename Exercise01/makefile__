CC = g++
CFLAGS = -Wall -O3 --std=c++11
INCS = -I../src
PTH = ../src/

main : main.o random.o blocking.o
	$(CC) $(PTH)random.o main.o -o main
main.o : main.cpp
	$(CC) -c main.cpp -o  main.o $(CFLAGS)
%.o : $(PTH)%.cpp $(PTH)%.h
	$(CC) -c $(PTH)%.cpp -o $(PTH)%.o $(CFLAGS)
clean :
	rm *.o main seed.out
